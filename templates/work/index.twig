{% extends "_layout" %}
{% block content %}
    {% set myEntryQuery = craft.entries()
    .section('work')
    .limit(10)
     %}
    {% set entries = myEntryQuery.all() %}
    {% for item in entries %}
        <div class="projectCard">
            {% include "work/_components/cards/" ~ item.type with item %}
        </div>
    {% endfor %}
{% endblock %}